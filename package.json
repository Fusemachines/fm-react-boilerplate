{
    "name": "fm-react-boilerplate",
    "version": "1.0.0",
    "description": "boiler plate for react project",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "commit": "git-cz"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Fusemachines/fm-react-boilerplate.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Fusemachines/fm-react-boilerplate/issues"
    },
    "homepage": "https://github.com/Fusemachines/fm-react-boilerplate#readme",
    "dependencies": {},
    "devDependencies": {
        "@commitlint/cli": "^8.1.0",
        "@commitlint/config-conventional": "^8.1.0",
        "commitizen": "^4.0.3",
        "cz-conventional-changelog": "^3.0.2",
        "husky": "^3.0.5"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
            "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
        }
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}